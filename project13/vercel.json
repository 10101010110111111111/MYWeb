{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "regions": ["fra1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "enabled": true,
    "silent": true
  }
}
